<?xml version="1.0"?>
<robot name="humanoid">
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.1 0.2 0.5"/>
      </geometry>
    </visual>
  </link>

  <link name="torso">
    <visual>
      <geometry>
        <box size="0.2 0.3 0.6"/>
      </geometry>
    </visual>
  </link>

  <joint name="base_to_torso" type="fixed">
    <parent link="base_link"/>
    <child link="torso"/>
  </joint>

  <!-- Define more links and joints for arms, legs, head -->

</robot>
